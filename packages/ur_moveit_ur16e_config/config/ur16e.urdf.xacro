<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="ur16e">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import ur16e urdf file -->
    <xacro:include filename="$(find recycle_bot)/urdf/ur16e.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="ur16e.ros2_control.xacro" />


    <xacro:ur16e_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
